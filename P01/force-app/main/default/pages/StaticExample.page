        <!-- Display images directly referenced in a static resource -->
        <h3>Images</h3>
        <p>Did not know you can do it :
            <apex:image alt="eye" title="eye"
                url="{!URLFOR($Resource.Assets, 'assets/images/product/medium-size/1-5.jpg')}"/>
            <apex:image alt="heart" title="heart"
                url="{!URLFOR($Resource.Assets, 'assets/images/product/medium-size/1-6.jpg')}"/>
